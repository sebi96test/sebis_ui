-- // StarterGui.ScreenGui.Frame.TextButton.LocalScript \\ --
local function SCRIPT_23()
local script = UI["23"]
	-- LocalScript inside the Noclip TextButton
	
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	
	local noclip = false
	
	local button = script.Parent
	
	local function setNoclip(state)
		noclip = state
		for _, part in ipairs(character:GetDescendants()) do
			if part:IsA("BasePart") then
				part.CanCollide = not state
			end
		end
		print("Noclip state set to: ", noclip)
	end
	
	local function toggleNoclip()
		setNoclip(not noclip)
	end
	
	button.MouseButton1Click:Connect(toggleNoclip)
	
	-- Initialize noclip state
	setNoclip(false)  -- Ensure noclip is initially off
	
end
task.spawn(SCRIPT_23)

return UI["1"], require;
