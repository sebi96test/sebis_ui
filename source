task.spawn(SCRIPT_17)
-- // StarterGui.ScreenGui.Frame.TextButton.LocalScript \\ --
local function SCRIPT_1a()
local script = UI["1a"]
	-- LocalScript inside the ESP TextButton
	
	local espButton = script.Parent
	local espEnabled = false
	
	local function createESP(player)
		local character = player.Character or player.CharacterAdded:Wait()
		local head = character:WaitForChild("Head")
	
		local billboardGui = Instance.new("BillboardGui")
		billboardGui.Name = "ESPLabel"
		billboardGui.Adornee = head
		billboardGui.Size = UDim2.new(1, 0, 1, 0)
		billboardGui.StudsOffset = Vector3.new(0, 2, 0)
		billboardGui.AlwaysOnTop = true
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Size = UDim2.new(1, 0, 1, 0)
		textLabel.BackgroundTransparency = 1
		textLabel.Text = player.Name
		textLabel.TextColor3 = Color3.new(1, 0, 0) -- Red color
		textLabel.TextScaled = true
		textLabel.Font = Enum.Font.SourceSansBold
	
		textLabel.Parent = billboardGui
		billboardGui.Parent = head
	end
	
	local function removeESP(player)
		local character = player.Character
		if character and character:FindFirstChild("Head") then
			local head = character.Head
			local espLabel = head:FindFirstChild("ESPLabel")
			if espLabel then
				espLabel:Destroy()
			end
		end
	end
	
	local function toggleESP()
		espEnabled = not espEnabled
	
		for _, player in pairs(game.Players:GetPlayers()) do
			if player ~= game.Players.LocalPlayer then
				if espEnabled then
					createESP(player)
				else
					removeESP(player)
				end
			end
		end
	end
	
	espButton.MouseButton1Click:Connect(toggleESP)
	
	game.Players.PlayerAdded:Connect(function(player)
		player.CharacterAdded:Connect(function(character)
			if espEnabled then
				createESP(player)
			end
		end)
	end)
	
	game.Players.PlayerRemoving:Connect(function(player)
		removeESP(player)
	end)
	
end
task.spawn(SCRIPT_1a)
